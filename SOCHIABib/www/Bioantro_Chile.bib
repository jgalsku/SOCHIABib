	file = {Full Text:C\:\\Users\\yo\\Zotero\\storage\\JM7CAB75\\LHeureux and Amorosi - 2010 - EL ENTIERRO DEL SITIO CERRO SOTA (MAGALLANES, CHIL.pdf:application/pdf;Full Text:C\:\\Users\\yo\\Zotero\\storage\\LSHK5WXP\\LHeureux and Amorosi - 2010 - EL ENTIERRO DEL SITIO CERRO SOTA (MAGALLANES, CHIL.pdf:application/pdf},
