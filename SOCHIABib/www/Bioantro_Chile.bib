	keywords = {mummies, molecular sequence data, DNA sequence analysis, emigration and immigration, bone marrow, viral DNA, globins, HTLV-I infections, human T-lymphotropic virus 1, proviruses, terminal repeat sequences},
	pages = {1753--1756},
}

@article{raghavan_genomic_2015,
	title = {Genomic evidence for the {Pleistocene} and recent population history of {Native} {Americans}},
	volume = {349},
	issn = {0036-8075},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4733658/},
	doi = {10.1126/science.aab3884},
	abstract = {How and when the Americas were populated remains contentious. Using ancient and modern genome-wide data, we find that the ancestors of all present-day Native Americans, including Athabascans and Amerindians, entered the Americas as a single migration wave from Siberia no earlier than 23 thousand years ago (KYA), and after no more than 8,000-year isolation period in Beringia. Following their arrival to the Americas, ancestral Native Americans diversified into two basal genetic branches around 13 KYA, one that is now dispersed across North and South America and the other is restricted to North America. Subsequent gene flow resulted in some Native Americans sharing ancestry with present-day East Asians (including Siberians) and, more distantly, Australo-Melanesians. Putative ‘Paleoamerican’ relict populations, including the historical Mexican Pericúes and South American Fuego-Patagonians, are not directly related to modern Australo-Melanesians as suggested by the Paleoamerican Model.},
	number = {6250},
	urldate = {2021-03-30},
	journal = {Science (New York, N.Y.)},
