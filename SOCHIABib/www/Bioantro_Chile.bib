@article{pellegrino-hurtado_reentierro_2021,
	title = {Reentierro de cuerpos humanos en el cementerio salitrero {Lagunas} ({Región} de {Tarapacá}, {Norte} de {Chile}). {Una} propuesta ética en el contexto de impacto ambiental},
	issn = {0719-1472},
	url = {https://revistadeantropologia.uchile.cl/index.php/RCA/article/view/64432},
	doi = {10.5354/0719-1472.2021.64432},
	abstract = {El hallazgo fortuito de restos humanos asociados a una ex oficina salitrera en el norte de Chile requirió el desarrollo de un plan de trabajo interdisciplinario para su tratamiento y disposición final. Este artículo explicita los criterios de intervención empleados para la conservación, el análisis bioantropológico y, finalmente, el reentierro de cada individuo identificado. Un objetivo central de este proyecto fue el tratamiento ético y la conservación de los restos humanos en actividades de impacto ambiental, lo cual constituye una propuesta innovadora en el contexto local. A través de este proceso, fue posible identificar la inexistencia de protocolos a nivel nacional que orienten el tratamiento técnico de los restos bioantropológicos, previo a su reentierro. Esto refleja la necesidad de desarrollar nuevos estándares más alineados con los valores culturales y expectativas de las comunidades.},
	number = {43},
	journal = {Revista Chilena de Antropología},
	author = {Pellegrino-Hurtado, Constanza and Herrera-Soto, María José and Opazo-Sepúlveda, Camila and Bugueño-Telias, Macarena},
	year = {2021},
	note = {Norte Grande;Bioarqueología,Manejo de cuerpos humanos},
	keywords = {conservación, cementerio histórico, cuerpos humanos, impacto ambiental, reentierro},
	pages = {19--58},
}

