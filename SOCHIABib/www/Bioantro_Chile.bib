	doi = {10.2458/azu_rc.57.18448},
	abstract = {From material culture evidence dating as early as 7500 cal BC, it has been established that populations from the interfluvic coast in northern Chile adapted to a maritime economic livelihood. During the 2nd millennium BC, local populations began to experience major social changes arising mainly from an increase in contacts with agropastoral populations from the highlands of the Andes. New radiocarbon data and stable isotope (δ
              15
              N
              col
              , δ
              13
              C
              col
              , and δ
              13
              C
              ap
              ) analyses of human bone remains from interfluvic coastal individuals were obtained. The data showed that these individuals, at the time of contact with highland populations, maintained a mode of subsistence relying principally on marine protein. This suggests that, although instances of social change may have arisen, the livelihoods linked to the consumption of marine resources would have remained constant, demonstrating a high degree of resistance in changing local lifestyles.},
	language = {en},
	number = {4},
	urldate = {2021-04-12},
	journal = {Radiocarbon},
	author = {Andrade, Pedro and Fernandes, Ricardo and Codjambassis, Katia and Urrea, Josefina and Olguín, Laura and Rebolledo, Sandra and Lira, Francisca and Aravena, Christian and Berríos, Mauricio},
