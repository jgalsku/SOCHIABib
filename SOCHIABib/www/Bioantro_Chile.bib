@article{retamal_evaluation_2011,
	title = {Evaluation of {Three} {Methods} of {Adult} {Age} {Estimation} {Based} on {Root} {Translucency} {Height}, {Periodontosis} {Height} and {Root} {Height} in a {Chilean} {Sample}},
	volume = {2},
	url = {https://www.researchgate.net/publication/264423539_Evaluation_of_Three_Methods_of_Adult_Age_Estimation_Based_on_Root_Translucency_Height_Periodontosis_Height_and_Root_Height_in_a_Chilean_Sample},
	abstract = {Using root transparency to estimate age at death has emerged as a useful method that is subject to minimal population variation. However, the adjust-ment of this method in target populations has proven necessary to achieve maximum accuracy. The purpose of this study is to evaluate three different methods of age estimation based on root transparency in a Chilean population sample constituted of 92 individuals (46 males and 46 females). Mean errors of estimation of the three methods show notable variation compared with documented age, particularly in the oldest individuals. The methods that obtained the most accurate results were the Prince and Ubelaker (2002) method for white males and the Lamendin et al. (1992) method for both sexes. These results suggest the need of creating specific formulas for the Chilean population in order to obtain more accurate estimations. indi-viduos (46 hombres y 46 mujeres). Los errores medios de la estimación de los tres métodos muestran una notable variación comparados con la edad documentada, particularmente en los individuos más ancianos. Los métodos que obtuvieron los resultados más certeros fueron el método de Prince y Ubelaker (2002) para hombres blancos y el método de Lamendin et al. (1992) para ambos sexos. Los resultados sugieren la necesidad de crear fórmulas específicas para la población chilena, con el fin de obtener estimaciones más certeras.},
	journal = {Forensic Oral Pathology Journal},
	author = {Retamal, Rodrigo and Ubelaker, Douglas},
	month = jul,
	year = {2011},
	note = {General;Forense,Osteología},
	keywords = {Chile, análisis de regresión, periodontosis, estimación de edad de muerte, transparencia radicular},
	pages = {16--19},
	file = {Full Text PDF:C\:\\Users\\yo\\Zotero\\storage\\BZVL3GM6\\Retamal and Ubelaker - 2011 - Evaluation of Three Methods of Adult Age Estimatio.pdf:application/pdf;Retamal and Ubelaker - Evaluation of Three Methods of Adult Age Estimatio.pdf:C\:\\Users\\yo\\Zotero\\storage\\UTT89WBJ\\Retamal and Ubelaker - Evaluation of Three Methods of Adult Age Estimatio.pdf:application/pdf},
}

